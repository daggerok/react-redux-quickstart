{
  "scripts": {
    "start": "webpack-dev-server --env development",
    "build": "webpack --env production -p",
    "serve": "npm-run-all build server",
    "server": "serve -Csp 3000 dist",
    "predocker-start": "docker build --rm --force-rm -t webapp -f config/nginx/Dockerfile . -f docker-nginx/Dockerfile",
    "docker-start": "docker run -d -p 80:80 --name webapp webapp",
    "docker-stop": "docker rm -f webapp | true",
    "compose-start": "docker-compose --project-name web -f docker-nginx/docker-compose.yml up -d",
    "compose-stop": "docker-compose --project-name web -f docker-nginx/docker-compose.yml down -v"
  },
  "dependencies": {
    "normalize.css": "^6.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router-dom": "4.0.0",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "next",
    "babel-preset-env": "1.3.2",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "base-href-webpack-plugin": "1.0.2",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.28.0",
    "html-webpack-plugin": "2.28.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "script-ext-html-webpack-plugin": "1.7.1",
    "serve": "5.1.2",
    "style-loader": "0.16.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
